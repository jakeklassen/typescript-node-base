{
  "name": "typescript-node-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./dist/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
    "prepare": "ts-patch install -s",
    "watch": "tsc --watch",
    "start": "esno src",
    "prod": "node dist",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jest/globals": "29.2.2",
    "@types/eslint": "8.4.8",
    "@types/node": "18.11.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "comment-json": "4.2.3",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "esno": "0.16.3",
    "expect": "29.2.2",
    "jest": "29.2.2",
    "jest-ts-auto-mock": "2.1.0",
    "prettier": "2.7.1",
    "shx": "0.3.4",
    "ts-auto-mock": "3.6.2",
    "ts-jest": "29.0.3",
    "ts-patch": "2.0.2",
    "tsm": "2.2.2",
    "typescript": "4.8.4"
  }
}
