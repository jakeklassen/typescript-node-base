{
  "name": "typescript-node-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./dist/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
    "prepare": "ts-patch install -s",
    "watch": "tsc --watch",
    "start": "esno src",
    "prod": "node dist",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jest/globals": "29.6.2",
    "@types/eslint": "8.44.2",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "comment-json": "4.2.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "esno": "0.17.0",
    "expect": "29.6.2",
    "jest": "29.6.2",
    "prettier": "3.0.1",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "ts-patch": "3.0.2",
    "tsm": "2.3.0",
    "typescript": "5.1.6"
  }
}
