{
  "name": "typescript-node-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./dist/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
    "prepare": "ts-patch install -s",
    "watch": "tsc --watch",
    "start": "esno src",
    "prod": "node dist",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@types/eslint": "8.21.1",
    "@types/node": "^18.15.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "comment-json": "4.2.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "esno": "0.16.3",
    "expect": "29.5.0",
    "jest": "29.5.0",
    "jest-ts-auto-mock": "2.1.0",
    "prettier": "2.8.4",
    "shx": "0.3.4",
    "ts-auto-mock": "3.6.4",
    "ts-jest": "29.0.5",
    "ts-patch": "2.1.0",
    "tsm": "2.3.0",
    "typescript": "4.9.5"
  }
}
