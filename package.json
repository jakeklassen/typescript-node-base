{
  "name": "typescript-node-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./dist/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
    "prepare": "ts-patch install -s",
    "watch": "tsc --watch",
    "start": "esno src",
    "prod": "node dist",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/eslint": "8.56.10",
    "@types/node": "^20.11.23",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "comment-json": "4.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "esno": "4.7.0",
    "expect": "29.7.0",
    "jest": "29.7.0",
    "prettier": "3.3.2",
    "shx": "0.3.4",
    "ts-jest": "29.1.5",
    "ts-patch": "3.2.0",
    "tsm": "2.3.0",
    "typescript": "5.4.5"
  }
}
