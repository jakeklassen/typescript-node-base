{
  "name": "typescript-node-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./src/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
    "prepare": "ts-patch install -s",
    "watch": "tsc --watch",
    "start": "esno src",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jest/globals": "28.1.0",
    "@types/eslint": "^8.4.2",
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "comment-json": "4.2.2",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "esno": "^0.16.3",
    "expect": "28.1.0",
    "jest": "28.1.0",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "2.6.2",
    "shx": "0.3.4",
    "ts-auto-mock": "3.6.2",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "ts-patch": "^2.0.1",
    "tsm": "2.2.1",
    "typescript": "4.7.2"
  }
}
